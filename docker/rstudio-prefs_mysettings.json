{
    "restore_source_documents": false,
    "save_workspace": "never",
    "load_workspace": false,
    "initial_working_directory": "~",
    "always_save_history": false,
    "restore_last_project": false,
    "show_invisibles": true,
    "highlight_r_function_calls": true,
    "rainbow_parentheses": true,
    "auto_append_newline": true,
    "code_completion_characters": 1,
    "default_encoding": "UTF-8",
    "indent_guides": "rainbowlines",
    "syntax_color_console": true,
    "font_size_points": 11,
    "editor_theme": "Cobalt",
    "server_editor_font_enabled": true,
    "server_editor_font": "UDEV Gothic LG",
    "posix_terminal_shell": "bash",
    "jobs_tab_visibility": "shown",
    "cran_mirror": {
        "name": "Japan (Tokyo) [https]",
        "host": "The Institute of Statistical Mathematics, Tokyo",
        "url": "https://cran.ism.ac.jp/",
        "country": "jp",
        "ok": 1,
        "secondary": ""
    },
    "rmd_viewer_type": "pane",
    "copilot_indexing_enabled": true,
    "copilot_completions_delay": 1,
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "Console",
            "TabSet2"
        ],
        "tabSet1": [
            "Presentation"
        ],
        "tabSet2": [
            "Environment",
            "History",
            "Files",
            "Plots",
            "Connections",
            "Packages",
            "Help",
            "Build",
            "VCS",
            "Tutorial",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 1
    },
    "graphics_backend": "ragg"
}
